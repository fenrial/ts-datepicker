{
  "name": "datepicker",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^2.4.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "sass": "^1.23.0",
    "@types/react": "16.8.6",
    "@types/react-dom": "16.8.2",
    "@types/node": "11.10.4"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "parcel-bundler": "^1.12.4",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "tslint": "^5.13.1",
    "tslint-react": "^3.6.0"
  },
  "scripts": {
    "start": "parcel index.html --open",
    "precommit": "lint-staged",
    "tslint": "node_modules/.bin/tslint -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run typecheck && yarn run precommit"
    }
  }
}
